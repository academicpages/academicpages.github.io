<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" default-locale="nl-NL" version="1.0" demote-non-dropping-particle="display-and-sort">
  <info>
    <title>De Buck (Dutch)</title>
