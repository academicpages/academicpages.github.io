from typing import Any

from django.core.handlers import base as base
from django.http import HttpRequest as HttpRequest

logger: Any

class ASGIRequest(HttpRequest):
    body_receive_timeout: int = ...
    scope: Any = ...
    resolver_match: Any = ...
    script_name: Any = ...
    path_info: Any = ...
    path: Any = ...
    method: Any = ...
