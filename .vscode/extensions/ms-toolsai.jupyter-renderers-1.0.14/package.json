		"@types/uuid": "^8.0.0",
		"@types/vscode": "^1.60.0",
		"@types/vscode-notebook-renderer": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^3.1.0",
		"@typescript-eslint/parser": "^3.1.0",
		"concurrently": "^5.2.0",
		"copy-webpack-plugin": "^6.0.2",
		"css-loader": "^3.5.3",
		"eslint": "^7.1.0",
